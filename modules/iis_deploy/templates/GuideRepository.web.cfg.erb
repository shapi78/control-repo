<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="fileRepository" type="Mobideo.Guide.Services.Common.Configuration.FileRepositoryConfigurationSection, Mobideo.Guide.Services.Common" />
