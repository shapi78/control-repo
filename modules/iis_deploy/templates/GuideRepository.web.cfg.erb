  <fileRepository>
    <repositories>
      <add name="AppFiles" />
      <add name="Attachment" />
      <add name="Archive" />
      <add name="Templates" />
      <add name="Versions" />
      <add name="Content" />
      <add name="Temp" />
      <add name="Reports" />
      <add name="Configuration" />
      <add name="Customer" />
    </repositories>
  </fileRepository>
  <appSettings>
    <add key="UseNewAuthentication" value="false" />
    <add key="ServiceAddress" value="https://<%=$serviceAddress%>"/>
    <add key="ServicesRootPath" value="GuideServices" />
    <add key="ConfigVersion" value="1.01" />
    <add key="RepositoryType" value="Guide" />
    <add key="AWSProfileName" />
  </appSettings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.6.1" />
      </system.Web>
  -->
  <system.web>
    <webServices>
      <protocols>
        <add name="HttpGet"/>
        <add name="HttpPost"/>
      </protocols>
    </webServices>
    <httpModules>
      <add name="BasicAuthHttpModule" type="WebApi.Authentication.BasicAuthenticationModule" />
    </httpModules>
    <compilation debug="true" targetFramework="4.6.1" />
    <httpRuntime targetFramework="4.5" maxRequestLength="40048576" requestPathInvalidCharacters="&lt;,&gt;,%,&amp;,:,\,?" />
    <authentication mode="None" />
    <pages validateRequest="false">
      <namespaces>
        <add namespace="System.Web.Helpers" />
        <add namespace="System.Web.Mvc" />
        <add namespace="System.Web.Mvc.Ajax" />
        <add namespace="System.Web.Mvc.Html" />
        <add namespace="System.Web.Optimization" />
        <add namespace="System.Web.Routing" />
        <add namespace="System.Web.WebPages" />
      </namespaces>
    </pages>
    <membership defaultProvider="Guide">
      <providers>
        <add name="Guide" type="Mobideo.Guide.Services.Client.GuideUMMembershipProvider, Mobideo.Guide.Services.Client" />
        <add name="GuideOldAuthentication" type="Mobideo.Guide.Services.Client.OldGuideUMMembershipProvider, Mobideo.Guide.Services.Client" />
      </providers>
    </membership>
    <roleManager enabled="true" defaultProvider="Guide">
      <providers>
        <add name="Guide" type="Mobideo.Guide.Services.Client.GuideUMRoleProvider, Mobideo.Guide.Services.Client" />
      </providers>
    </roleManager>
    <sessionState timeout="60" />
  </system.web>
  <system.webServer>
    <security>
      <requestFiltering>
        <requestLimits maxAllowedContentLength="50000000" />
      </requestFiltering>
    </security>
    <validation validateIntegratedModeConfiguration="false" />
    <handlers>    
      <add name="StaticHandler" path="Css/*" verb="GET" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
      <add name="StaticHandler2" path="Images/*" verb="GET" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    <remove name="ExtensionlessUrlHandler-Integrated-4.0" /><remove name="OPTIONSVerbHandler" /><remove name="TRACEVerbHandler" /><add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" /></handlers>
    <modules runAllManagedModulesForAllRequests="true">
      <add name="BasicAuthHttpModule" type="WebApi.Authentication.BasicAuthenticationModule" />
    </modules>
  </system.webServer>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
